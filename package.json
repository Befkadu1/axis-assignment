{
  "name": "applicant-backend-test",
  "version": "1.0.0",
  "description": "App for verification of assignment",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test",
    "start": "rimraf ./dist && tsc && nodemon dist/index.js",
    "build": "tsc",
    "compile": "yarn build && node dist/index.js",
    "test:all": "tsc && nyc mocha --exit dist/**/*.spec.js"
  },
  "nyc": {
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 50,
    "sourceMap": true,
    "exclude": [
      "**/*.spec.js",
      "**/test",
      "**/enums",
      "**/models"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.8.8",
    "chai": "^4.2.0",
    "jsonwebtoken": "^9.0.0",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-bodyparser": "^4.2.1",
    "koa-pino-logger": "^2.1.3",
    "koa-router": "^7.4.0",
    "mocha": "^6.1.4",
    "pino": "^5.12.6",
    "request": "^2.88.0",
    "request-promise": "^4.1.4",
    "rimraf": "^2.6.3",
    "sqlite3": "^4.0.9",
    "typescript": "^3.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@koa/cors": "^2.2.3",
    "@types/kcors": "^2.2.3",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.40",
    "eslint": "^5.16.0"
  }
}
